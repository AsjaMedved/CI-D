     "cy:folder2": "cypress run --spec 'cypress/e2e/folder2/**/*.cy.js'",
     
     "cy:parallel-folders": "npx npm-run-all --parallel cy:folder1 cy:folder2",

     "cy:Admin": "cypress run --group admin --parallel --ci-build-id 2 --spec cypress/e2e/folder1/Admin.cy.js --record --key c9361249-ac10-40b7-b490-80765431f2fd",
     "cy:Client": "cypress run --group client --parallel --ci-build-id 2 --spec cypress/e2e/folder1/Client.cy.js --record --key c9361249-ac10-40b7-b490-80765431f2fd",
     "cy:Films": "cypress run --group films --parallel --ci-build-id 2 --spec cypress/e2e/folder1/Films.cy.js --record --key c9361249-ac10-40b7-b490-80765431f2fd",
     "cy:Cinema": "cypress run --group —Åinema --parallel --ci-build-id 2 --spec cypress/e2e/folder2/LetsGoToTheCinema.cy.js --record --key c9361249-ac10-40b7-b490-80765431f2fd",

     "cy:all-parallel": "npx npm-run-all --parallel cy:Admin cy:Client cy:Films cy:Cinema"
},
  
